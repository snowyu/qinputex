{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "indent": [
      true
    ],
    "interface-name": [
      false
    ],
    "member-access": false,
    "no-console": true,
    "no-empty-interface": false,
    "arrow-parens": false,
    "object-literal-key-quotes": false,
    // Pending fix for shorthand property names.
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
